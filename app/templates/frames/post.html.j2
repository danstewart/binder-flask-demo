<div class="post">
    <div>
        <p>{{ post.content }}</p>
        <a href="/user/{{ post.author.id }}">@{{ post.author.name }}</a>
    </div>

    <div>
        <button @click.eval="this.loadUrl('/frames/post/edit/{{ post.id }}')">Edit</button>
        <button @click.eval="this.loadUrl(`/frames/post/delete/{{ post.id }}`)">Delete</button>
    </div>
</div>
